@{
    ViewBag.Title = "Home Page";
}
@{ 
    List<string> Dogs = (List<string>)  ViewData["MyDogs"];
    var Cats = ViewBag.MyCats;
    
}
<h2>View Data aka ViewBag with a Key</h2>
<div>@ViewData["MyDogs"]</div>

<h2>You Flithy ViewBag</h2>
<div>@ViewBag.MyCats</div>

<h2>Cats List</h2>
<ul>
@foreach(string c in Cats)
{
    <li>@c</li>
}
    </ul>

<h2>Dogs List</h2>
<ul>
    @for(int i = 0; i < Dogs.Count; i++)
    {
        <li>@Dogs[i]</li>
    }
</ul>


